@using Ubik.Web.Auth.ViewModels
@{
    var isDetail = false;
    if (Model.GetType() == typeof (RoleViewModel))
    {
        isDetail = true;
        var detail = Model as RoleViewModel;
        Html.RenderPartial("~/Areas/Backoffice/Views/UserAdministration/Partials/RoleDetails.cshtml", detail);
    }
    else
    {
        var rows = Model as IEnumerable<RoleViewModel>;
        Html.RenderPartial("~/Areas/Backoffice/Views/UserAdministration/Partials/RoleList.cshtml", rows);
    }
}

